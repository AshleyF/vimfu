  "setup": [
    {
      "comment": "Kill any stale tmux sessions"
    },
    {
      "line": "tmux kill-server 2>/dev/null; true"
    },
    {
      "comment": "Set TERM for proper color support"
    },
    {
      "line": "export TERM=xterm-256color"
    },
    {
      "comment": "Prepare workspace"
    },
    {
      "line": "mkdir -p ~/vimfu && cd ~/vimfu"
    },
    {
      "line": "rm -f fetcher.py .fetcher.py.swp .fetcher.py.swo"
    },
    {
      "line": "rm -f ~/.local/state/nvim/swap/*fetcher.py*"
    },
    {
      "comment": "Create demo files"
    },
    {
      "writeFile": "fetcher.py",
      "content": "import asyncio\n\nasync def fetch(url):\n    reader, writer = await (\n        asyncio.open_connection(\n            url, 443, ssl=True))\n    writer.write(b\"GET / \")\n    data = await reader.read()\n    return data.decode()\n"
    },
    {
      "writeFile": "tests.txt",
      "content": "PASS test_fetch.py\n  ✓ connects to host    3ms\n  ✓ reads response     2ms\n  ✓ decodes utf8       1ms\n\n3 passed in 0.06s\n"
    },
    {
      "writeFile": "logs.txt",
      "content": "14:22:01 INFO  worker start\n14:22:02 INFO  fetch begin\n14:22:02 WARN  slow dns 210ms\n14:22:03 INFO  fetch done\n14:22:03 INFO  cache updated\n"
    },
    {
      "line": "clear"
    },
    {
      "comment": "Launch tmux"
    },
    {
      "line": "tmux"
    },
    {
      "waitForScreen": "0:zsh",
      "timeout": 30.0
    },
    {
      "comment": "Window 0 — project overview"
    },
    {
      "line": "clear && ls -la"
    },
    {
      "comment": "Window 1 — code in nvim (real editor)"
    },
    {
      "keys": "\u0002"
    },
    {
      "keys": "c"
    },
    {
      "waitForScreen": "1:zsh",
      "timeout": 5.0
    },
    {
      "line": "nvim fetcher.py"
    },
    {
      "ifScreen": "swap file",
      "thenKeys": "d"
    },
    {
      "waitForScreen": "asyncio",
      "timeout": 30.0
    },
    {
      "comment": "Window 2 — test results"
    },
    {
      "keys": "\u0002"
    },
    {
      "keys": "c"
    },
    {
      "waitForScreen": "2:zsh",
      "timeout": 5.0
    },
    {
      "line": "clear && cat tests.txt"
    },
    {
      "comment": "Window 3 — log stream"
    },
    {
      "keys": "\u0002"
    },
    {
      "keys": "c"
    },
    {
      "waitForScreen": "3:zsh",
      "timeout": 5.0
    },
    {
      "line": "clear && cat logs.txt"
    },
    {
      "comment": "Go back to window 0"
    },
    {
      "keys": "\u0002"
    },
    {
      "keys": "0"
    },
    {
      "waitForScreen": "0:zsh",
      "timeout": 5.0
    }
  ],
